  },
  "homepage": "https://github.com/nolannaphys/maze-mania#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.1"
  }
